{
  "name": "stylesnames",
  "description": "Util to conditionally apply Stylesheets for React and React-Native.",
  "version": "1.1.2",
  "repository": "github:tobua/stylesnames",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "start": "padua watch",
    "test": "padua test"
  },
  "main": "dist/index.js",
  "source": "index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "stylesnames",
    "styles",
    "classnames",
    "react",
    "react-native"
  ],
  "devDependencies": {
    "padua": "0.1.2"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/padua/configuration/.babelrc"
        }
      ]
    }
  },
  "engines": {
    "node": ">= 13.2.0"
  }
}
